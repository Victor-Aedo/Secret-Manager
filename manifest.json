{
    "manifest_version": 3,
    "name": "Secret Manager",
    "description": "Password manager",
    "version": "1.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/padlock.png"
    },
    "content_scripts": [
        {
          "js": ["popup/popup.js"],
          "matches": [
            "https://developer.chrome.com/docs/extensions/*",
            "https://developer.chrome.com/docs/webstore/*"
          ]
        }
      ]
        
      
  }